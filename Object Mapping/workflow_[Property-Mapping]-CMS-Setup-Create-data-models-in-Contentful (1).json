   \"validations\": [\n                        {\n                            \"linkContentType\": [\n                            \"sourceSystem\"\n                            ]\n                        }\n                    ],\n                    \"linkType\": \"Entry\"\n                }\n            },\n            {\n                \"id\": \"canonicalPropertyMapping\",\n                \"name\": \"Canonical Property Mapping\",\n                \"type\": \"Link\",\n                \"localized\": false,\n                \"required\": false,\n                \"validations\": [\n                    {\n                        \"linkContentType\": [\n                            \"sourceProperty\"\n                        ]\n                    }\n                ],\n                \"disabled\": false,\n                \"omitted\": false,\n                \"linkType\": \"Entry\"\n            }\n        ]\n    },\n    {\n        \"id\":\"sourceObject\",\n        \"name\": \"Source Object\",\n        \"description\": \"An object for a given source system.\",\n        \"displayField\": \"friendlyName\",\n        \"fields\": [\n            {\n                \"id\": \"friendlyName\",\n                \"name\": \"Friendly Name\",\n                \"type\": \"Symbol\",\n                \"localized\": false,\n                \"required\": false,\n                \"validations\": [ \n                  {\n                    \"unique\": true\n                  }\n                ],\n                \"disabled\": false,\n                \"omitted\": false\n            },\n            {\n                \"id\": \"systemName\",\n                \"name\": \"System Name\",\n                \"type\": \"Symbol\",\n                \"localized\": false,\n                \"required\": false,\n                \"validations\": [],\n                \"disabled\": false,\n                \"omitted\": false\n            },\n            {\n                \"id\": \"properties\",\n          